  "permissions": [
    "notifications",
    "storage",
    "background"
  ],
  "shortcuts": [
    {
      "name": "My Playlists",
      "short_name": "Collections",
      "description": "Manage your Music Collection",
      "url": "/myCollections",
      "icons": [
        {
          "src": "./icons/topTracks.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Discover",
      "short_name": "Discover",
      "description": "Discover New Artists",
      "url": "/allArtists",
      "icons": [
        {
          "src": "./icons/favoriteMusic.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "edge_side_panel": {
    "preferred_width": 200
  },
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "name",
      "text": "description",
      "url": "link",
      "files": [
        {
          "name": "file",
          "accept": [
            "image/*",
            "video/*"
          ]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "audio/*": [
          ".mp3",
          ".wav",
          ".ogg"
        ]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+mybeats",
      "url": "/open-protocol?url=%s"
    }
  ]
}
}
